<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Rental.DomainModel" namespace="Rental">
  <class name="RentalInformation, Rental.DomainModel" lazy="false" table="RentalInformation">
    <id name="Id" column="Id">
      <generator class="native"/>
    </id>
    <property name="From" column="From"/>
    <property name="To" column="To"/>
    <property name="DailyCost" column="DailyCost"/> 
    <many-to-one name="Rented" class="Rental, Rental.DomainModel" column="Rented"/>
    <many-to-one name="Client" class="Rental, Rental.DomainModel" column="Client"/>
  </class>
</hibernate-mapping>