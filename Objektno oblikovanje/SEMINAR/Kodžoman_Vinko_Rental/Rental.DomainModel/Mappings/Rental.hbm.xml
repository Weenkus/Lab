<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Rental.DomainModel" namespace="Rental">
  <class name="Rental" lazy="false" table="Rental">
    <id name="Id" column="Id">
      <generator class="native"/>
    </id>
    <discriminator column="RentalType"/>
    <property name="Name" column="Name"/>
    <property name="Description" column="Description"/>
    <property name="DailyPrice" column="DailyPrice"/>   
    <many-to-one name="Owner" class="Rental, Rental" column="Owner"/>
    
    <subclass name="Apartment" discriminator-value="Apartment" lazy="false"></subclass>
  </class>
</hibernate-mapping>